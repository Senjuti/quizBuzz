{
  "name": "redux-simple-starter",
  "version": "1.0.0",
  "description": "Simple starter package for Redux with React and Babel support",
  "main": "index.js",
  "repository": "git@github.com:StephenGrider/ReduxSimpleStarter.git",
  "scripts": {
    "build": "webpack --mode development",
    "copy-assets": "@powershell copy -r '%INIT_CWD%\\assets\\' '%INIT_CWD%\\dist\\assets\\' && @powershell copy -r '%INIT_CWD%\\style\\' '%INIT_CWD%\\dist\\style\\'",
    "start": "npm run build && webpack serve --mode development",
    "deploy": "npm run build && npm run copy-assets && gh-pages -d ./dist"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "babel-loader": "^9.1.2",
    "chai": "^4.3.7",
    "chai-jquery": "^2.1.0",
    "css-loader": "^6.7.3",
    "gh-pages": "^5.0.0",
    "html-webpack-plugin": "^5.5.0",
    "jquery": "^3.6.4",
    "jsdom": "^21.1.1",
    "mocha": "^10.2.0",
    "style-loader": "^3.3.2",
    "webpack": "^5.78.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.2"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "axios": "^1.3.4",
    "body-parser": "^1.20.2",
    "dotenv": "^16.0.3",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-keydown": "^1.9.12",
    "react-redux": "^8.0.5",
    "react-router": "^6.9.0",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.6.0"
  }
}
